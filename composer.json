{
    "name": "ahmeti/core",
    "description": "Ahmeti Core",
    "homepage": "https://ahmetimamoglu.com.tr",
    "keywords": [
        "ahmeti",
        "core"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ahmet Imamoglu",
            "email": "ahmet363@gmail.com",
            "role": "Developer",
            "homepage": "https://ahmetimamoglu.com.tr"
        }
    ],
    "require": {},
    "require-dev": {},
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Ahmeti\\Core\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "aliases": {
                "Core": "App\\Core",
                "Form": "App\\Form",
                "Response": "App\\Response"
            },
            "providers": [
                "Ahmeti\\Core\\Providers\\CoreServiceProvider",
                "Ahmeti\\Core\\Providers\\FormServiceProvider",
                "Ahmeti\\Core\\Providers\\ResponseServiceProvider"
            ]
        }
    }
}